global proc string getAOV()
{

string $the_AOVs_arr[];
clear $the_AOVs_arr;
int $num_AOVs=0;
if(`objExists defaultArnoldDriver`)
{
string $listC_arnold[]=`listConnections  defaultArnoldDriver`;

for ($crs4=0;$crs4<`size($listC_arnold)`;$crs4++){
