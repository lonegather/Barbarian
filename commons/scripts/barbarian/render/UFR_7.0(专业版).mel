global proc int browseFolder_rl(int $mode,string $dirpath,string $type) 
{
	
	if ($mode==0){
		 textField -e -text (toNativePath($dirpath)+"\\") path_loc;
		return true;	
	}
	
	else if  ($mode==1){
		 textField -e -text (toNativePath($dirpath)) path_loc;
		return true;	
		
	}
}

